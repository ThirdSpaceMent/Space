@model IEnumerable<prjWebSpaceMent.ViewModels.RatingViewModel>
@{
    ViewBag.Title = "ShowRating";
    if (User.Identity.Name == "")
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_LayoutMember.cshtml";
    }
}
<!--頁首 Section-->
<section id="subspaces">
    <div class="container wow fadeInUp">
        <div class="section-header p-5">
            <h2>瀏覽其他場地評論？</h2>
            <p>點擊下方按鈕回到找場地！</p>
        </div>
        <div class="AnimatedButton">
            <a class="AnimatedButton-a" style="--clr:#22e622;--i:2;" href="/Spaces/Spaces_Index"><span class="AnimatedButton-a-span">找場地</span></a>
        </div>
    </div>
</section>

<main>
    <div class="section-header p-3 mb-3">
        <h2>評價</h2>
        <p>以下是其他會員對此場地的評價</p>
    </div>
    <div class="mb-5" style="word-break:break-all;">
        @{ if (Model.Count() == 0)
            {
                <p class="d-flex justify-content-center">目前尚未有會員對此場地進行評論!</p>
            }
            else
            {
                foreach (var item in Model)
                {
                    @Html.Partial("_CommentPartial", item)
                }
            }
        }
    </div>
    <div class="d-flex justify-content-center">
        <button class="btn btn-danger mb-4 mr-4" onclick="javascript:window.history.go(-1);">返回</button>
    </div>
    <textarea rows="6" cols="50" style="display:none;" id="a3361"></textarea>
    <div>
        <input type="button" id="buttonAqqw" value="click"/>
    </div>
</main>

<script type="text/javascript">
