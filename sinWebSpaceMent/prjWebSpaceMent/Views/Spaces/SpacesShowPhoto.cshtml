@model IEnumerable<prjWebSpaceMent.Models.Spaces>
<!DOCTYPE html>
<html lang="zh-Hant-TW">
<!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <title>
        場地照片
    </title>
    <!-- Bootstrap core CSS -->
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/FAQ.css" rel="stylesheet">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-H0GPP781SL"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-H0GPP781SL');
    </script>
</head>
<body>
    <main>

        <!-- 開頭圖片 -->
        <div class="faq">
            <section class="kv-search d-flex justify-content-center">
                <div class="align-self-center">
                    <h1 class="h1-static text-center mb-md-4 mb-3 wow fadeInDown">場地照片總覽</h1>
                </div>
            </section>
        </div>
        <div class="d-flex justify-content-center mb-3 mt-4">
            <button class="btn btn-success" type="button" onclick="history.back()">回到上一頁</button>
        </div>
        <table class="table border-0 mr-0 ml-0 d-flex justify-content-center" style="word-break:keep-all;">
            <tr>
                <th class="text-center">場地序號</th>
                <th class="text-center">場地名稱</th>
                <th class="text-center">場地照片</th>
            </tr>
            @{
                int count = 0;
                foreach (var item in Model)
                {
                    count++;
                    <tr>
                        <td class="text-center">@count</td>
                        <td  class="text-center">@Html.DisplayFor(modelItem => item.sName)</td>
                        <td class="container">
                            <!--<div class="d-flex justify-content-center mt-3">-->
                            @*<h1 class="text-center"><br /> <span class="badge badge-info" style="font-size:16px"></span></h1>*@
                            <!--</div>-->
                            <a href="~/img/gallery/@item.sPhoto" class="venobox" data-gall="gallery-carousel"><img src="~/img/gallery/@item.sPhoto" alt="" style="width:300px;height:300px"></a>
                        </td>
                        <!--<td>-->
                        @*@Html.ActionLink("Edit", "Edit", new { id = item.sNumber }) |
                            @Html.ActionLink("Details", "Details", new { id = item.sNumber }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.sNumber })*@
                        <!--</td>-->
                    </tr>
                }
            }
        </table>
    </main>
</body>
</html>
